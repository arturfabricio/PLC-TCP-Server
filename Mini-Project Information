require(['jquery'], function($) {
    $('#single_select5fc9fdce9c0514').change(function() {
        var ignore = $(this).find(':selected').attr('data-ignore');
        if (typeof ignore === typeof undefined) {
            $('#single_select_f5fc9fdce9c0513').submit();
        }
    });
});
